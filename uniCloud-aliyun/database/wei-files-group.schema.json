// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [ "name" ],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
    "name": {
      "bsonType": "string",
      "description": "分组名称"
    },
    "create_date": {
    	"bsonType": "timestamp",
    	"description": "创建时间",
      "forceDefaultValue": {
      	"$env": "now"
      }
    }
	}
}